<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- â†³ Label -->
    <TextView
        android:id="@+id/label"
        android:layout_width="168dp"
        android:layout_height="18dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:gravity="right|top"
        android:lineSpacingExtra="2sp"
        android:text="@string/data_home"
        android:textAppearance="@style/data_home"
        android:translationY="-1.24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/title"
        android:layout_width="96dp"
        android:layout_height="41dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:gravity="end|top"
        android:lineSpacingExtra="0sp"
        android:text="@string/today_home"
        android:textAppearance="@style/today_home"
        android:translationY="-0.21sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label"
        android:layout_alignParentStart="true" />

    <!-- Rectangle 1 -->
    <View
        android:id="@+id/home_photo"
        android:layout_width="382dp"
        android:layout_height="230dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/home_photo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <!-- Other events -->
    <TextView
        android:id="@+id/other_event"
        android:layout_width="284dp"
        android:layout_height="41dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="30dp"
        android:gravity="top"
        android:lineSpacingExtra="0sp"
        android:text="@string/other_event"
        android:textAppearance="@style/other_event"
        android:translationY="-0.21sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_photo" />

    <!-- Rectangle 1 -->
    <View
        android:id="@+id/home_photo2"
        android:layout_width="382dp"
        android:layout_height="230dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/home_photo2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/other_event" />


    <!-- Software engineering Seminar -->
    <TextView
        android:id="@+id/software_en"
        android:layout_width="276dp"
        android:layout_height="56dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="1dp"
        android:gravity="top"
        android:lineSpacingExtra="2sp"
        android:text="@string/software_en"
        android:textAppearance="@style/software_en"
        android:translationY="-0.87sp"
        app:layout_constraintStart_toStartOf="@+id/home_photo"
        app:layout_constraintTop_toBottomOf="@+id/nurlan_shai" />


    <!-- Nurlan Shaidullaev -->
    <TextView
        android:id="@+id/nurlan_shai"
        android:layout_width="196dp"
        android:layout_height="20dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="16dp"
        android:gravity="top"
        android:lineSpacingExtra="2sp"

        android:text="@string/nurlan_shai"
        android:textAppearance="@style/nurlan_shai"
        android:translationY="-1.05sp"
        app:layout_constraintStart_toStartOf="@+id/home_photo"
        app:layout_constraintTop_toTopOf="@+id/home_photo" />

    <!-- 11:00-13:00 -->
    <TextView
        android:id="@+id/some_id_profile"
        android:layout_width="80dp"
        android:layout_height="15dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginBottom="16dp"
        android:gravity="bottom"
        android:text="@string/time"
        android:textAppearance="@style/time"
        app:layout_constraintBottom_toBottomOf="@+id/home_photo"
        app:layout_constraintStart_toStartOf="@+id/home_photo" />

    <View
        android:id="@+id/vector"
        android:layout_width="11dp"
        android:layout_height="15dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="86dp"
        android:layout_marginRight="102dp"
        android:background="@drawable/map"
        app:layout_constraintEnd_toEndOf="@+id/home_photo"
        app:layout_constraintBottom_toBottomOf="@+id/home_photo" />

    <!-- A309 -->
    <TextView
        android:id="@+id/a309"
        android:layout_width="91dp"
        android:layout_height="15dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:gravity="right|top"
        android:text="@string/a309"
        android:textAppearance="@style/a309"
        app:layout_constraintEnd_toEndOf="@+id/home_photo"
        app:layout_constraintBottom_toBottomOf="@+id/home_photo" />

    <!-- Oleg Dzhumanaliev -->
    <TextView
        android:id="@+id/oleg_dzhuma"
        android:layout_width="196dp"
        android:layout_height="20dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="16dp"
        android:gravity="top"
        android:lineSpacingExtra="2sp"
        android:text="@string/oleg_dzhuma"
        android:textAppearance="@style/oleg_dzhuma"
        android:translationY="-1.05sp"
        app:layout_constraintStart_toStartOf="@+id/home_photo2"
        app:layout_constraintTop_toTopOf="@+id/home_photo2" />

    <!-- Study abroad Seminar -->
    <TextView
        android:id="@+id/study_abroa"
        android:layout_width="276dp"
        android:layout_height="56dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="1dp"
        android:gravity="top"
        android:lineSpacingExtra="2sp"
        android:text="@string/study_abroa"
        android:textAppearance="@style/study_abroa"
        android:translationY="-0.87sp"
        app:layout_constraintStart_toStartOf="@+id/home_photo2"
        app:layout_constraintTop_toBottomOf="@+id/oleg_dzhuma" />

    <!-- Tomorrow 15:00-17:00 -->
    <TextView
        android:id="@+id/tomorrow_15"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginBottom="16dp"
        android:gravity="bottom"
        android:text="@string/tomorrow_15"
        android:textAppearance="@style/tomorrow_15"
        app:layout_constraintBottom_toBottomOf="@+id/home_photo2"
        app:layout_constraintStart_toStartOf="@+id/home_photo2" />


    <View
        android:id="@+id/vector2"
        android:layout_width="11dp"
        android:layout_height="15dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="86dp"
        android:layout_marginRight="102dp"
        android:background="@drawable/map"
        app:layout_constraintEnd_toEndOf="@+id/home_photo2"
        app:layout_constraintBottom_toBottomOf="@+id/home_photo2" />


    <!-- Konya Hall -->
    <TextView
        android:id="@+id/konya_hall"
        android:layout_width="91dp"
        android:layout_height="15dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="19dp"
        android:text="@string/konya_hall"
        android:textAppearance="@style/konya_hall"
        android:gravity="end|top"
        app:layout_constraintEnd_toEndOf="@+id/home_photo2"
        app:layout_constraintBottom_toBottomOf="@+id/home_photo2"
        android:layout_marginRight="19dp" />

    <View
        android:id="@+id/home_photo3"
        android:layout_width="382dp"
        android:layout_height="230dp"
        android:background="@drawable/home_photo3"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_photo2"
        android:layout_marginLeft="16dp" />

    <!-- Olga Kan -->
    <TextView
        android:id="@+id/olga_kan"
        android:layout_width="196dp"
        android:layout_height="20dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="@+id/home_photo3"
        app:layout_constraintStart_toStartOf="@+id/home_photo3"
        android:text="@string/olga_kan"
        android:textAppearance="@style/olga_kan"
        android:lineSpacingExtra="2sp"
        android:translationY="-1.05sp"
        android:gravity="top"
        android:layout_marginLeft="19dp" />


    <!-- SMM -->
    <TextView
        android:id="@+id/smm"
        android:layout_width="276dp"
        android:layout_height="28dp"
        android:text="@string/smm"
        android:textAppearance="@style/smm"
        android:lineSpacingExtra="2sp"
        android:translationY="-0.87sp"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="19dp"
        app:layout_constraintStart_toStartOf="@+id/home_photo3"
        app:layout_constraintTop_toBottomOf="@+id/olga_kan"
        android:gravity="top"
        />








</androidx.constraintlayout.widget.ConstraintLayout>